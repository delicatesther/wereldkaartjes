{
  "name": "wereldplaatjes",
  "dependencies": {
    "gulp": "*",
    "gulp-sass": "*",
    "gulp-util": "*",
    "run-sequence": "*",
    "gulp-sourcemaps": "*",
    "gulp-if": "*",
    "gulp-autoprefixer": "*",
    "gulp-merge-media-queries": "*",
    "gulp.spritesmith": "*",
    "merge-stream": "*",
    "gulp-iconfont-css": "*",
    "gulp-iconfont": "*",
    "gulp-cssfont64": "*",
    "gulp-svg-sprite": "*",
    "gulp-uglify": "*"
  },
  "paths": {
    "sass": {
      "src": "src/sass/",
      "dest": "dest/css/"
    },
    "js": {
      "src": "src/js/",
      "dest": "dest/js/"
    },
    "iconfont": {
      "base": "dest/",
      "conf": "src/conf/",
      "src": "src/img/iconfont/",
      "destScss": "../../src/sass/04-autogenerated/",
      "destFont": "dest/fonts/",
      "destFontRelative": "../fonts/"
    },
    "base64css": {
      "src": "dest/fonts/",
      "destcss": "src/sass/04-autogenerated/"
    },
    "spritePng": {
      "src": "src/img/sprite-png/",
      "src_retina": "src/img/sprite-png/sprite2x/",
      "dest_css": "src/sass/04-autogenerated/",
      "destRelative": "../img/",
      "dest": "dest/img/"
    },
    "spriteSvg": {
      "srcRoot": "src/",
      "conf": "src/conf/",
      "src": "src/img/sprite-svg/",
      "destScss": "sass/04-autogenerated/",
      "destSvg": "../dest/img/"
    }
  }
}
